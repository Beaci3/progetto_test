    <div class="col-md-12">


        @Ajax.EncodedAjaxActionLink("Nuovo Consulente / Sportello Datoriale", "Anagrafica", "ConsulenteCs", null, new AjaxOptions
        {
        OnBegin = "alertWaid()",
