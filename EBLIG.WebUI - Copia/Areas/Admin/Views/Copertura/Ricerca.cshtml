@model  EBLIG.WebUI.Areas.Admin.Models.CoperturaModel

<div class="row mb-4">
    <div class="col-md-12">
        @Ajax.EncodedAjaxActionLink("Nuova Copertura", "Nuovo", "Copertura", null, new AjaxOptions
                {
                OnBegin = "alertWaid()",
                OnSuccess = "showModalFullScreen_NoHeaderFooter(data); alertClose()",
                OnFailure = "handleError"
                }, new { @class = "btn btn-info" })
    </div>
</div>

@Html.RicercaModulo(Html.Partial("RicercaModulo", Model), "Ricerca Copertura")

@Html.UpdateListRicerca(Url.Action("Ricerca", "Copertura"), true)
