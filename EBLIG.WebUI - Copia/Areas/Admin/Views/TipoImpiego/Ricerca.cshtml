@model  EBLIG.WebUI.Areas.Admin.Models.TipoImpiegoModel

<div class="row mb-4">
    <div class="col-md-12">
        @Ajax.EncodedAjaxActionLink("Nuovo Tipo Impiego", "Nuovo", "TipoImpiego", null, new AjaxOptions
                {
                OnBegin = "alertWaid()",
                OnSuccess = "showModalFullScreen_NoHeaderFooter(data); alertClose()",
                OnFailure = "handleError"
                }, new { @class = "btn btn-info" })
    </div>
</div>

@Html.RicercaModulo(Html.Partial("RicercaModulo", Model), "Ricerca Tipo Impiego")

@Html.UpdateListRicerca(Url.Action("Ricerca", "TipoImpiego"), true)
